<h3>Sites List</h3>
@if (Sites?.Any() == true)
{
    foreach (var site in Sites)
    {
        <div>
            <h2>@site.Title</h2>
            <a href="@site.Url"> @site.Url</a>
        </div>
    }

}
else
{
    <p>No sites available</p>

}


@code {

    [Parameter]
    public List<Site> Sites { get; set; }
}
